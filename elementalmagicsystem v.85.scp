[ITEMDEF i_curse_timer]
ID=i_memory
NAME=Curse Timer
TYPE=t_eq_script

CATEGORY=Miscellaneous
SUBSECTION=Memory Items
DESCRIPTION=Curse Timer

ON=@Create
ATTR attr_invis
MORE {20 100}

ON=@Equip
TIMER 1
SRC.SPELLEFFECT s_paralyze 1000

ON=@Timer
IF <CONT>
IF <MORE>
IF <CONT.HITS>>0
MORE <MORE>+-1
CONT.HITS <CONT.HITS>+-1
CONT.SOUND snd_spell_mana_drain
CONT.EFFECT 3,0375a,6,15,1
TIMER 1
RETURN 1
ELSE
CONT.SYSMESSAGE You are no longer cursed.
ENDIF
ENDIF
ENDIF
REMOVE
RETURN 1

[ITEMDEF i_rot_timer]
ID=i_worldgem_bit
NAME=Rot Timer
TYPE=t_eq_script
LAYER=layer_special

CATEGORY=Miscellaneous
SUBSECTION=Memory Items
DESCRIPTION=Undead Decay Timer

ON=@Create
ATTR attr_invis
MORE1 {20 100}

ON=@Equip
TIMER 1
SRC.SPELLEFFECT s_paralyze 1000

ON=@Timer
IF <CONT>
IF <MORE1>
MORE1 <MORE1>+-1
CONT.HITPOINTS <CONT.HITPOINTS>+-1
CONT.SOUND snd_spell_mana_drain
CONT.EFFECT 3,0375a,6,15,1
TIMER 1
RETURN 1
ELSE
CONT.SYSMESSAGE You are no longer afflicted by the undead decay.
ENDIF
ENDIF
REMOVE
RETURN 1

[ITEMDEF i_elem_default]
ID=i_memory
NAME=Elemental Tag Setter
TYPE=t_eq_script

CATEGORY=Miscellaneous
SUBSECTION=Memory Items
DESCRIPTION=Elemental Tag Setter

ON=@Create
ATTR attr_invis
TIMER 180

ON=@Equip
TARGET Who's elemental resistance levels would you like to set to the default level?
RETURN 0

ON=@TargOn_Item
TARGET
RETURN 1

ON=@TargOn_Char
IF set_resist
SRC.TARG.TAG.EARTH 1
SRC.TARG.TAG.WATER 1
