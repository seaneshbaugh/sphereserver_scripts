[ITEMDEF i_flood_tile_2]
ID=i_water
NAME=Water
TYPE=t_normal

CATEGORY=Miscellaneous
SUBSECTION=Spell Effects
DESCRIPTION=Flood Tile

ON=@Create
ATTR attr_can_decay|attr_static
TIMER 1

ON=@Timer
REMOVE
RETURN 1

ON=@Step
IF 0<SRC.TAG.WATER><=0
SRC.DAMAGE (<LINK.MAGERY>/(4.0))
SRC.DAMAGE {0 10}
ELSEIF 0<SRC.TAG.WATER>==1
IF <SRC.MAGICRESISTANCE>>RAND(<LINK.MAGERY>)
SRC.SYSMESSAGE You feel yourself resisting magical energy.
LINK.SYSMESSAGE They seem to resist the spells effects.
SRC.DAMAGE (<SRC.TARG.MAGICRESISTANCE>/30.0)
SRC.DAMAGE {0 4}
SRC.f_skillgain MAGICRESISTANCE
ELSE
SRC.DAMAGE <LINK.MAGERY>/8.0
SRC.DAMAGE {0 10}
ENDIF
ELSEIF <SRC.TAG.WATER>==2
IF <SRC.MAGICRESISTANCE>>RAND(<LINK.MAGERY>/2.0)
SRC.SYSMESSAGE You feel yourself resisting magical energy.
LINK.SYSMESSAGE They seem to resist the spells effects.
SRC.DAMAGE (<SRC.TARG.MAGICRESISTANCE>/30.0)
SRC.DAMAGE {0 4}
SRC.f_skillgain MAGICRESISTANCE
ELSE
SRC.DAMAGE <LINK.MAGERY>/16.0
SRC.DAMAGE {0 10}
ENDIF
ELSEIF 0<SRC.TAG.WATER>==3
SRC.DAMAGE 0
ELSEIF 0<SRC.TAG.WATER>>=4
SRC.HITS <SRCHITS>+(<LINK.MAGERY>/4.0)
ENDIF
IF <SRC.FLAGS>&statf_freeze
SRC.FLAGS <SRC.FLAGS>&~statf_freeze
ENDIF
RETURN 1

