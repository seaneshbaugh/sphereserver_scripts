[FUNCTION f_find_width]
IF <VAR.ENDX><<VAR.STARTX>
VAR.TEMP <VAR.STARTX>
VAR.STARTX <VAR.ENDX>
VAR.ENDX <VAR.TEMP>
VAR.TEMP
ENDIF
IF (<VAR.STARTX><0)&&(<VAR.ENDX><0)
RETURN <EVAL (<absolutevalue <VAR.STARTX>>+-<absolutevalue <VAR.ENDX>>)>
ELSEIF (<VAR.STARTX>>0)&&(<VAR.ENDX>>0)
RETURN <EVAL (<VAR.ENDX>+-<VAR.STARTX>)
ELSEIF (<VAR.STARTX><0)&&(<VAR.ENDX>>0)
RETURN <EVAL (<absolutevalue <VAR.STARTX>>+<VAR.ENDX>)>
ELSEIF (VAR.ENDX><0)&&(<VAR.STARTX>>0)
RETURN <EVAL (<absolutevalue <VAR.ENDX>>+<VAR.STARTX>)>
ENDIF

[FUNCTION f_find_length]
IF <VAR.ENDY><<VAR.STARTY>
VAR.TEMP <VAR.STARTY>
VAR.STARTY <VAR.ENDY>
VAR.ENDY <VAR.TEMP>
VAR.TEMP
ENDIF
IF (<VAR.STARTY><0)&&(<VAR.ENDY><0)
RETURN <EVAL (<absolutevalue <VAR.STARTY>>+-<absolutevalue <VAR.ENDY>>)>
ELSEIF (<VAR.STARTY>>0)&&(<VAR.ENDY>>0)
RETURN <EVAL (<VAR.ENDY>+-<VAR.STARTY>)
ELSEIF (<VAR.STARTY><0)&&(<VAR.ENDY>>0)
RETURN <EVAL (<absolutevalue <VAR.STARTY>>+<VAR.ENDY>)>
ELSEIF (VAR.ENDY><0)&&(<VAR.STARTY>>0)
RETURN <EVAL (<absolutevalue <VAR.ENDY>>+<VAR.STARTY>)>
ENDIF

[FUNCTION f_tile_item]
IF !0<ARGS>
RETURN 1
