[ITEMDEF i_monster_skill_l3_poison]
ID=i_memory
NAME=L3 Poison Skill (Player Version)
TYPE=t_eq_script

CATEGORY=Miscellaneous
SUBSECTION=Memory Items
DESCRIPTION=L3 Poison Skill (Player Version)

ON=@Create
ATTR attr_invis

ON=@Equip
IF (<SRC.FLAGS>&statf_dead)||(<SRC.FLAGS>&statf_freeze)||(<SRC.FLAGS>&statf_sleeping)||(<SRC.FLAGS>&statf_polymorph)||(<SRC.FLAGS>&statf_incognito)||(<SRC.FLAGS>&statf_stone)||(<SRC.FLAGS>&statf_hallucinating)
SRC.SYSMESSAGE You cannot use this in your current state.
REMOVE
RETURN 1
ENDIF
IF <SRC.MANA><20
SRC.SYSMESSAGE You lack enough mana to use this ability.
REMOVE
RETURN 1
ENDIF
TARGET Select who you wish to poison.
RETURN 0

ON=@TargOn_Item
SRC.SYSMESSAGE That is not a valid target.
REMOVE
RETURN 1

ON=@TargOn_Char
IF !<SRC.TARG.CANSEELOS>
SRC.SYSMESSAGE You cannot see them.
REMOVE
RETURN 1
ENDIF
IF <SRC.TARG.DISTANCE>>15
SRC.SYSMESSAGE They are too far away.
REMOVE
RETURN 1
ENDIF
SRC.MANA <SRC.MANA>+-20
IF <EVAL <SRC.TARG.STR>+-(3*(<SRC.TARG.STR>/3))>
SRC.TARG.SPELLEFFECT s_poison 300.0
ELSE
SRC.SYSMESSAGE The poison seems to have no effect on them.
ENDIF
REMOVE
RETURN 1

