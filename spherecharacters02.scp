SKILLMAKE=CARPENTRY 100.0,t_carpentry
RESOURCES=i_log 10,i_ingot_iron 10

ON=@Create
COLOR=0835

ON=@DCLICK
IF !<SRC.MAGERY>>=100.0
SAYU You are not skilled enough to use this.
RETURN 1
ELSEIF !(RAND(3)) && (<SRC.RESTEST 10 i_golem_stone> )
SRC.EMOTE The conditions were not right and the Golem fell appart after creation.
SRC.CONSUME=10 i_golem_stone
SRC.CONSUME=i_stone_golem_kit
RETURN 1
ELSEIF (<SRC.RESTEST 10 i_golem_stone> )
SRC.CONSUME=10 i_golem_stone
SRC.CONSUME=i_stone_golem_kit
SRC.NEWITEM=i_stonegolem_figurine
SRC.ACT.BOUNCE
SRC.ACT.DCLICK
RETURN 1
ELSE
SRC.SYSMESSAGE You need 10 Golem Stone to use this.
RETURN 1
ENDIF

