[CHARDEF c_dummy]
ID=c_man_invisible
NAME=Invisible Dummy

CATEGORY=Miscellaneous
SUBSECTION=Other Bodies
DESCRIPTION=Dummy Body
 
ON=@Create
FLAGS statf_invisible

ON=@Click
RETURN 1

ON=@Death
HITS <STR>
RETURN 1

[FUNCTION chkmove]
ACT.P <ACT.HOME>
ACT.MOVE <ARGS>
VAR.CHKPX <ACT.P.X>
VAR.CHKPY <ACT.P.Y>
ACT.P <ACT.HOME>
ACT.WALK <ARGS>
IF (<VAR.CHKPX> == <ACT.P.X> ) && (<VAR.CHKPY> == <ACT.P.Y> )
ACT.HOME <ACT.P>
VAR.MOVES <EVAL 0<VAR.MOVES> + 1>
IF (<VAR.DIST> > 1)
VAR.DIST <EVAL <VAR.DIST> - 1>
chkmove <ARGS>
ENDIF
RETURN 1
ENDIF
RETURN 0

